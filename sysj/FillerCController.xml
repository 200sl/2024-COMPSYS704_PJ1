<System xmlns="http://systemjtechnology.com">
    <SubSystem Name="FILLC" Local="true">
        <ClockDomain Name="FillerCControllerCD" Class="FillerCController">
            <iSignal Name="bottleAtPos2C"                Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="40004" />
            <iSignal Name="dosUnitCEvac"                 Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="40004" />
            <iSignal Name="dosUnitCAtTarget"             Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="40004" />
            <iSignal Name="bottleAtPos2CFull"            Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="40004" />


            <oSignal Name="valveInjectorCOnOff"      To="FillerModel.valveInjectorCOnOff"   Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="41010" />
            <oSignal Name="valveInletCOnOff"         To="FillerModel.valveInletCOnOff"      Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="41011" />
            <oSignal Name="dosUnitCValveRetract"     To="FillerModel.dosUnitCValveRetract"  Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="41012" />
            <oSignal Name="dosUnitCValveExtend"      To="FillerModel.dosUnitCValveExtend"   Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="41013" />


            <!--            signal for system coordinator  -->
            <iSignal Name="fillerCDoProcess"        Class="com.systemj.ipc.SimpleServer"    IP="127.0.0.1" Port="40004"/>

            <oSignal Name="fillerCIdle"             To="Coordinator.fillerCIdle"          Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="43003" />
        </ClockDomain>
    </SubSystem>
</System>
